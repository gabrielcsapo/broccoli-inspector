    "@storybook/ember-cli-storybook": "^0.2.1",
    "@types/ember": "^3.1.2",
    "@types/ember-qunit": "^3.4.8",
    "@types/ember__test-helpers": "^0.7.10",
    "@types/qunit": "^2.9.1",
    "@types/rsvp": "^4.0.3",
    "babel-eslint": "^10.1.0",
