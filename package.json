{
  "name": "broccoli-inspector",
  "version": "0.2.5",
  "description": "🔍 inspect what's really happening in your broccoli pipeline",
  "repository": "https://github.com/gabrielcsapo/broccoli-inspector.git",
  "license": "MIT",
  "author": "Gabriel J. Csapo",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "EMBER_CLI_INSTRUMENTATION=1 ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "storybook": "start-storybook -p 6006 -s dist",
    "build-storybook": "build-storybook -s dist",
    "storybook-dev": "npm-run-all --aggregate-output --continue-on-error --parallel start storybook",
    "prepublish": "yarn build"
  },
  "files": [
    "dist",
    "lib"
  ],
  "dependencies": {
    "apollo-server-express": "^2.12.0",
    "ember-cli-version-checker": "^5.1.1",
    "express": "^4.17.1",
    "find-up": "^4.1.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.3",
    "socket.io": "^2.3.0"
  },
